{
  "name": "ticketchain-frontend",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host --port 3000",
    "prebuild": "pnpm run copy-artifacts",
    "copy-artifacts": "mkdir -p public/artifacts/contracts/TicketNFT.sol && cp ../artifacts/contracts/TicketNFT.sol/TicketNFT.json public/artifacts/contracts/TicketNFT.sol/TicketNFT.json",
    "build": "tsc --project tsconfig.json && vite build",
    "lint": "eslint '../frontend/src/**/*.{ts,tsx}' --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@rainbow-me/rainbowkit": "^1.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "ethers": "^5.7.2",
    "lucide-react": "^0.288.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "sonner": "^1.2.0",
    "styled-components": "^6.1.17",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "viem": "^1.18.0",
    "wagmi": "^1.4.5"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.8.10",
    "@types/prop-types": "^15.7.5",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "@vitest/coverage-v8": "^0.34.6",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6"
  }
}
